<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rps_trial1">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_white_on_black"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_white_on_black"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SinglePlayerInfo" />
        <activity android:name=".SinglePlayer" />
        <activity android:name=".PlayerInfo" />
        <activity
            android:name=".Double"
            android:screenOrientation="fullSensor" />
        <activity android:name=".MainActivity">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>